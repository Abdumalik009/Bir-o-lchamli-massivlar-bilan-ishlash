package Array;

import java.util.Scanner;

public class Array8 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Massivni kiriting: ");
        int n = scanner.nextInt();

        int[] array = new int[n];
        for (int i = 0; i < n; i++) {
            System.out.print("Massiv uzini kiriting: ");
            array[i] = scanner.nextInt();
        }


        toq(array);

    }
    public static void toq(int[] array) {
        System.out.print("Massiv elementlari orasidan toqlar: ");
        for (int i = 0; i < array.length; i++) {
            if (i % 2 != 0) {
                System.out.print(array[i] + " ");
            }
        }
    }
}

package Array;

import java.util.Scanner;

public class Array8 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Massivni kiriting: ");
        int n = scanner.nextInt();

        int[] array = new int[n];
        for (int i = 0; i < n; i++) {
            System.out.print("Massiv uzini kiriting: ");
            array[i] = scanner.nextInt();
        }


        toq(array);

    }
    public static void toq(int[] array) {
        System.out.print("Massiv elementlari orasidan toqlar: ");
        for (int i = 0; i < array.length; i++) {
            if (i % 2 != 0) {
                System.out.print(array[i] + " ");
            }
        }
    }
}

package Array;

import java.util.Scanner;

public class Array8 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Massivni kiriting: ");
        int n = scanner.nextInt();

        int[] array = new int[n];
        for (int i = 0; i < n; i++) {
            System.out.print("Massiv uzini kiriting: ");
            array[i] = scanner.nextInt();
        }


        toq(array);

    }
    public static void toq(int[] array) {
        System.out.print("Massiv elementlari orasidan toqlar: ");
        for (int i = 0; i < array.length; i++) {
            if (i % 2 != 0) {
                System.out.print(array[i] + " ");
            }
        }
    }
}

