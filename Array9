public class Array9 {
    public static void main(String[] args) {
        int[] massiv = {4, 5, 7, 8, 6, 9};

        System.out.print("Massiv elementlari orasidan juftlar: ");
        for (int i = massiv.length - 1; i >= 0; i -= 2) {
            System.out.print(massiv[i] + " ");
        }

        int juftlarSoni = massiv.length / 2;
        System.out.println("\nJuftlar soni = " + juftlarSoni);
    }
}
